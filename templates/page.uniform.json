\n\n    \/* ─── Image side ────────────────────────────────── *\/\n    .split-screen .image img {\n      width: 100%;\n      height: 100%;\n      object-fit: cover;\n      display: block;\n    }\n\n    \/* ─── Responsive ───────────────────────────────── *\/\n  @media (max-width: 768px) {\n  .split-screen {\n    flex-direction: column;\n    position: relative;\n    height: auto;\n  }\n  .split-screen .text {\n    position: absolute;\n    top: 85%;\n    left: 45%;\n    transform: translate(-50%, -50%);\n    width: 100%;\n    text-align: center;\n    pointer-events: none;\n  }\n  .split-screen .image {\n    order: 1;\n  }\n  .saadaa-for {\n    font-size: 24px;\n    white-space: nowrap;\n    color: #fff;\n    text-shadow: 0 2px 4px rgba(0,0,0,0.4);\n    text-align: left;\n  }\n  .saadaa-for .static,\n  .saadaa-for .clip {\n    display: inline-block !important;   \/* Inline for mobile *\/\n    vertical-align: middle;\n    font-size: 24px;\n    margin: 0;\n    height: 26px;\n    white-space: nowrap;\n  }\n  .saadaa-for .static {\n    font-weight: normal;\n   \n  }\n  .saadaa-for .clip {\n    width: 8ch;\n    overflow: hidden;\n    height: 29px;\n    position: relative;\n    text-align: left;\nmargin-bottom\n  }\n  .saadaa-for .dynamic {\n    display: inline-block !important;\n    font-size: 24px;\n    font-weight: 700;\n    line-height: 26px;\n    position: relative;\n    vertical-align: middle;\n  }\n}\n\n  <\/style>\n<\/head>\n<body>\n\n  <div class=\"split-screen\">\n    <!-- LEFT: animated text -->\n    <div class=\"text\">\n      <div class=\"saadaa-for\">\n        <span class=\"static\">Saadaa for<\/span>\n        <span class=\"clip\">\n          <span class=\"dynamic\">Work<\/span>\n        <\/span>\n      <\/div>\n    <\/div>\n\n    <!-- RIGHT: your image -->\n    <div class=\"image\">\n      <img\n        src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0450\/3476\/6485\/files\/Group_1171275337.png?v=1747211064\"\n        alt=\"Saadaa for\">\n    <\/div>\n  <\/div>\n\n  <script>\n    document.addEventListener('DOMContentLoaded', () => {\n      const words    = ['Work', 'School', 'Hospital', 'Everyone'];\n      let idx        = 0;\n      const el       = document.querySelector('.saadaa-for .dynamic');\n      const duration = 400;    \/\/ animation duration in ms\n      const pause    = 800;    \/\/ pause between cycles in ms\n\n      function slideUp() {\n        el.classList.add('slide-out');\n        el.addEventListener('animationend', function handleOut() {\n          el.removeEventListener('animationend', handleOut);\n          idx = (idx + 1) % words.length;\n          el.textContent = words[idx];\n          el.classList.remove('slide-out');\n          el.classList.add('slide-in');\n          el.addEventListener('animationend', function handleIn() {\n            el.removeEventListener('animationend', handleIn);\n            el.classList.remove('slide-in');\n          });\n        });\n      }\n\n      setTimeout(() => {\n        slideUp();\n        setInterval(slideUp, duration * 2 + pause);\n      }, pause);\n    });\n  <\/script>\n\n<\/body>\n<\/html>","text_alignment":"center"}},"custom_liquid_EdTGA4":{"type":"custom-liquid","name":"t:sections.custom_liquid.presets.custom_liquid.name","settings":{"separate_section_with_border":true,"remove_vertical_spacing":true,"remove_horizontal_spacing":true,"liquid":"<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n  <meta charset=\"UTF-8\"\/>\n  <meta name=\"viewport\" content=\"width=device-width,initial-scale=1\"\/>\n  <title>Sadabahar Offers<\/title>\n  <style>\n    \/* ─── mobile-first (updated with unique class names) ───────────────────────── *\/\n    .sadab-offer-unique {\n      max-width: 100%;\n      margin: auto;\n      padding: 3rem 2rem;\n      text-align: center;\n    }\n\n    \/* first line on mobile *\/\n    .sadab-offer-unique-title {\n      font-size: 18px;\n      color: #333;\n      font-weight: 300;\n      margin: 0;\n      line-height: 35px;\n      display: block; 