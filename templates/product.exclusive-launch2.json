an class=\"pd-sep\">:<\/span>\n        <span class=\"pd-num\" id=\"pd-hours\">00<\/span><span class=\"pd-unit\">h<\/span>\n        <span class=\"pd-sep\">:<\/span>\n        <span class=\"pd-num\" id=\"pd-minutes\">00<\/span><span class=\"pd-unit\">m<\/span>\n        <span class=\"pd-sep\">:<\/span>\n        <span class=\"pd-num\" id=\"pd-seconds\">00<\/span><span class=\"pd-unit\">s<\/span>\n      <\/div>\n    <\/div>\n\n    <!-- body -->\n    <div class=\"pd-offer-body\">\n      <div class=\"pd-left\">\n        <div class=\"pd-order\">Order Now<\/div>\n        <div class=\"pd-subtitle\">Act Fast – One-time Only!<\/div>\n      <\/div>\n      <div class=\"pd-right\">\n        <div class=\"pd-prices\">\n          <span class=\"pd-mrp\">MRP ₹2,199<\/span>\n          <span class=\"pd-price\">₹1,699<\/span>\n        <\/div>\n        <div class=\"pd-off\">₹500 OFF<\/div>\n      <\/div>\n    <\/div>\n  <\/div>\n\n<script>\n  (function(){\n    const now = new Date();\n    const year = now.getFullYear();\n    \/\/ JS months: 0-based (June=5)\n    const start = new Date(year, 5, 28, 11, 0, 0); \/\/ 28 June 11:00 AM\n    const end   = new Date(year, 5, 30, 11, 0, 0); \/\/ 30 June 11:00 AM\n\n    function updateCountdown() {\n      const now = new Date();\n      let diff;\n      if (now < start) {\n        \/\/ Before start: show full time window\n        diff = end - start;\n      } else if (now >= start && now <= end) {\n        \/\/ Between start and end: show countdown\n        diff = end - now;\n      } else {\n        \/\/ After end: show zero\n        diff = 0;\n      }\n      const days    = Math.floor(diff \/ 86400000);\n      const hours   = Math.floor((diff % 86400000) \/ 3600000);\n      const minutes = Math.floor((diff % 3600000)  \/ 60000);\n      const seconds = Math.floor((diff % 60000)    \/ 1000);\n\n      document.getElementById('pd-days').textContent    = String(days   ).padStart(2,'0');\n      document.getElementById('pd-hours').textContent   = String(hours  ).padStart(2,'0');\n      document.getElementById('pd-minutes').textContent = String(minutes).padStart(2,'0');\n      document.getElementById('pd-seconds').textContent = String(seconds).padStart(2,'0');\n    }\n    updateCountdown();\n    setInterval(updateCountdown, 1000);\n  })();\n<\/script>\n\n\n\n<\/body>\n<\/html>"}},"liquid_VR9prd":{"type":"liquid","settings":{"liquid":"{%- comment -%}\n  Normalize product title:\n  - Downcase\n  - Replace line breaks with space\n  - Replace multiple spaces with a single space\n{%- endcomment -%}\n{% assign normalized_title = product.title | downcase | replace: '\n', ' ' | replace: '  ', ' ' | replace: '  ', ' ' %}\n\n{%- comment -%}\n  Detect shirt and sleeve type\n{%- endcomment -%}\n{% assign is_shirt = false %}\n{% assign is_full_sleeve = false %}\n{% assign is_half_sleeve = false %}\n{% assign is_kurta = false %}\n{% assign is_short_kurta = false %}\n{% assign is_long_kurta = false %}\n\n{%- comment -%}\n  Full Sleeve Shirt: match '100% linen shirt'\n{%- endcomment -%}\n{% if normalized_title contains '100% linen shirt' %}\n  {% assign is_shirt = true %}\n  {% assign is_full_sleeve = true %}\n{% endif %}\n\n{%- comment -%}\n  Half Sleeve Shirt: match 'half sleeve shirt'\n{%- endcomment -%}\n{% if normalized_title contains 'half sleeve shirt' %}\n  {% assign is_shirt = true %}\n  {% assign is_half_sleeve = true %}\n{% endif %}\n\n{%- comment -%}\n  Short\/Long Kurta: match\n{%- endcomment -%}\n{% if normalized_title contains 'short kurta' %}\n  {% assign is_kurta = true %}\n  {% assign is_short_kurta = true %}\n{% endif %}\n{% if normalized_title contains 'long kurta' %}\n  {% assign is_kurta = true %}\n  {% assign is_long_kurta = true %}\n{% endif %}\n\n{%- comment -%}\n  Get toggle links from metafields (adjust as needed)\n{%- endcomment -%}\n{% assign full_sleeve_link = product.metafields.custom.full_sleeve_toggle_link | strip %}\n{% assign half_sleeve_link = product.metafields.custom.half_sleeve_toggle_link | strip 