iles\/1\/0450\/3476\/6485\/files\/Group_1171275160.webp?v=1754391117\" alt=\"Product 3\" \/>\n            <\/div>\n          <\/div>\n        <\/a>\n        <!-- CARD 4 -->\n        <a class=\"pc-carousel-link\" href=\"https:\/\/saadaa.in\/collections\/everyday-cotton-top\" target=\"_blank\">\n          <div class=\"pc-carousel-slide\">\n            <div class=\"pc-product-image-wrapper\">\n              <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0450\/3476\/6485\/files\/Group_1171275162.webp?v=1754391117\" alt=\"Product 4\" \/>\n            <\/div>\n          <\/div>\n        <\/a>\n        <!-- CARD 5 -->\n        <a class=\"pc-carousel-link\" href=\"https:\/\/saadaa.in\/collections\/airy-linen-shirts\" target=\"_blank\">\n          <div class=\"pc-carousel-slide\">\n            <div class=\"pc-product-image-wrapper\">\n              <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0450\/3476\/6485\/files\/Group_1171275163.webp?v=1754391118\" alt=\"Product 5\" \/>\n            <\/div>\n          <\/div>\n        <\/a>\n      \n\n      <\/div>\n\n      <!-- RIGHT ARROW -->\n      <button class=\"pc-carousel-arrow pc-arrow-right\" id=\"pc-arrow-right\">\n        <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\">\n          <path d=\"M9 6l6 6-6 6\" stroke=\"#666\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"\/>\n        <\/svg>\n      <\/button>\n    <\/div>\n  <\/div>\n  <!-- ========== END CAROUSEL SECTION ========== -->\n\n  <script>\n    document.addEventListener('DOMContentLoaded', () => {\n      const track = document.getElementById('pc-carousel');\n      const outer = document.getElementById('pc-carousel-outer');\n      const slides = Array.from(track.children);\n      const btnLeft = document.getElementById('pc-arrow-left');\n      const btnRight = document.getElementById('pc-arrow-right');\n\n      let currentIndex = 0;\n      let slideWidth = 0;\n      let gap = 12;\n      let perView = 1;\n\n      functi