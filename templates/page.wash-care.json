   .item .wrapper {\n      position: relative;\n      display: inline-block;\n      padding: var(--gap);\n      border: 2px solid transparent;\n      border-radius: 50%;\n      transition: border 0.2s;\n    }\n    .item img {\n      width: var(--size);\n      height: var(--size);\n      object-fit: cover;\n      border-radius: 50%;\n      display: block;\n    }\n    .item.active .wrapper {\n      border-color: var(--accent);\n      border-width: 1px;\n    }\n    .item .label {\n      margin-top: 0.5rem;\n      font-size: 0.95rem;\n      color: #444;\n      transition: color 0.2s;\n      \/* no white-space: nowrap; *\/\n   width: 88px;      \/* force same width as the circle *\/\n      margin-left: auto;         \/* center under the circle *\/\n      margin-right: auto;\n      text-align: center;        \/* center multi-word labels *\/\n      line-height: 1.2;          \/* allow wrapping with some breathing room *\/\n    }\n    .item.active .label {\n      color: var(--accent);\n    }\n    .item .check {\n      display: none;\n      position: absolute;\n      bottom: 4px;\n      right: 4px;\n      width: 18px;\n      height: 18px;\n      background: var(--accent);\n      border-radius: 50%;\n      padding: 2px;\n      border: 1.5px solid #fff;\n    }\n    .item.active .check {\n      display: block;\n    }\n\n    \/* Hide arrows entirely (never show) *\/\n    .carousel-arrow {\n      display: none;\n    }\n\n    \/* --- Material Info Layout --- *\/\n    #material-info {\n      display: flex;\n      gap: 2.5rem;\n      justify-content: center;\n      align-items: flex-start;\n      max-width: 1100px;\n      margin: 0 auto;\n      border-radius: 2rem;\n      padding-bottom: 2.5rem;\n    }\n    #care-description {\n      flex: 1.2;\n      font-size: 1.07rem;\n      padding-right: 1rem;\n      min-width: 220px;\n      text-align: left;\n    }\n    #care-description p {\n      margin: 1.1em 0 0.5em;\n      line-height: 1.6;\n    }\n    #care-description strong {\n      color: #3a3a3a;\n      font-weight: 600;\n    }\n    #care-description em {\n      color: #99715b;\n      font-size: 0.96em;\n      margin-top: 1em;\n      display: block;\n    }\n    #video-section {\n      flex: 1;\n      min-width: 250px;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n    }\n    #material-video {\n      width: 320px;\n      max-width: 100%;\n      border-radius: 1.5rem;\n      background: #eae6e2;\n      box-shadow: 0 2px 10px #0001;\n    }\n\n    \/* --- Responsive for Mobile --- *\/\n    @media (max-width: 900px) {\n      #material-info {\n        flex-direction: column;\n        gap: 1.6rem;\n        border-radius: 1.3rem;\n      }\n    }\n    @media (max-width: 640px) {\n      .carousel {\n        gap: 1rem;\n        padding-bottom: 0.5rem;\n        margin-bottom: 1.5rem;\njustify-content:flex-start;\n      }\nh2 {\n    \n    margin-bottom: 1.5rem;\n}\n      .item {\n        flex: 0 0 24%; \/* roughly 4 items visible, 5th peeks in *\/\n        max-width: 24%;\n      }\n      .item img {\n        width: 100%;\n        height: auto;\n      }\n      .item .label {\n        margin-top: 0.4rem;\n        font-size: 0.8rem;\n        \/* allow wrapping on mobile as well *\/\n        width: 100%;\n        text-align: center;\n      }\n      .carousel-container {\n        overflow: hidden;\n        margin-bottom: 1.5rem;\n      }\n    }\n  <\/style>\n<\/head>\n<body>\n  <div class=\"main-bg\">\n    <h2>Different care for different materials<\/h2>\n\n    <!-- Carousel container (arrows never appear) -->\n    <div class=\"carousel-container\">\n      <div class=\"carousel\" id=\"carousel\">\n        <div class=\"item active\">\n          <div class=\"wrapper\">\n            <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0450\/3476\/6485\/files\/Frame_67_4.png?v=1749100290\" alt=\"100% Cotton\">\n            <svg class=\"check\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"#fff\" stroke-width=\"3\" stroke-linecap=\"ro