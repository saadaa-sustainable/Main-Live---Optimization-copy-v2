poster;\n      video.load();\n      washingEl.textContent = mat.washing;\n      dryingEl.textContent = mat.drying;\n      ironingEl.textContent = mat.ironing;\n      noteEl.textContent = mat.note;\n    }\n\n    items.forEach((item, idx) => {\n      item.addEventListener('click', () => {\n        items.forEach(i => i.classList.remove('active'));\n        item.classList.add('active');\n        updateMaterial(idx);\n      });\n    });\n\n    \/\/ Initialize with first material\n    updateMaterial(0);\n  <\/script>\n<\/body>\n<\/html>","text_alignment":"center"}},"media_grid_carousal_BnizFr":{"type":"media-grid-carousal","blocks":{"image_Tcem7W":{"type":"image","settings":{"image":"shopify:\/\/shop_images\/Screenshot_2025-06-02_at_2.55.33_PM_1_1.png","title":"","heading_tag":"h4","content":"","image_below_content":"<p>A bucket for hand-washing<\/p>","below_text_position":"start","icon_width":10,"link_text":"","link_url":"","link_style":"outline","text_position":"place-self-end-start text-start","text_color":"#ffffff","button_background":"rgba(0,0,0,0)","button_text_color":"rgba(0,0,0,0)","overlay_color":"#1c1c1c","overlay_opacity":0}},"image_3jiDeN":{"type":"image","settings":{"image":"shopify:\/\/shop_images\/Screenshot_2025-06-02_at_2.56.49_PM_1.png","title":"","heading_tag":"h4","content":"","image_below_content":"<p>Gentle detergent<\/p>","below_text_position":"start","icon_width":10,"link_text":"","link_url":"","link_style":"outline","text_position":"place-self-end-start text-start","text_color":"#ffffff","button_background":"rgba(0,0,0,0)","button_text_color":"rgba(0,0,0,0)","overlay_color":"#1c1c1c","overlay_opacity":0}},"image_C837qU":{"type":"image","settings":{"image":"shopify:\/\/shop_images\/Screenshot_2025-06-02_at_2.57.59_PM_1.png","title":"","heading_tag":"h4","content":"","image_below_content":"<p>A drying rack<\/p>","below_text_position":"start","icon_width":10,"link_text":"","link_url":"","link_style":"outline","text_position":"place-self-end-start tex