<script>
(() => {
  const badge = document.getElementById('discountBadge-{{ product.id }}');
  if (!badge) return;

  function render(priceCents, compareCents){
    const p = Number(priceCents) || 0;
    const c = Number(compareCents) || 0;
    if (c > p && p > 0) {
      const pct = Math.round(((c - p) * 100) / c);
      badge.textContent = pct + '% OFF';
      badge.hidden = false;
    } else {
      badge.hidden = true;
    }
    badge.dataset.priceCents = p;
    badge.dataset.compareCents = c;
  }

  // Initial state from Liquid
  render(badge.dataset.priceCents, badge.dataset.compareCents);

  // Update from theme variant events
  function updateFromVariant(v){
    if (!v) return;
    render(v.price, v.compare_at_price || 0);
  }

  document.addEventListener('variant:change', e => updateFromVariant(e?.detail?.variant));
  document.addEventListener('product:variant-change', e => updateFromVariant(e?.detail?.variant));

  // Fallback: observe the variant <select> using Product JSON
  const variantSelect = document.querySelector('form[action*="/cart/add"] [name="id"]');
  const jsonEl = document.querySelector('script[id^="ProductJson-"]') || document.getElementById('ProductJson-Manual');
  let productJson = null;
  if (jsonEl?.textContent) { try { productJson = JSON.parse(jsonEl.textContent); } catch(e){} }

  if (variantSelect && productJson?.variants?.length) {
    variantSelect.addEventListener('change', () => {
      const id = String(variantSelect.value);
      const v  = productJson.variants.find(x => String(x.id) === id);
      updateFromVariant(v);
    });
  }
})();
</script>


