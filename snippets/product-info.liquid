<script>
(function() {
  var link   = document.getElementById('payday-banner-link');
  var overlay= document.getElementById('payday-overlay');
  var closeB = overlay.querySelector('.payday-close');
  var cta    = document.getElementById('payday-cta');
  var lastFocused = null;

  // Open modal
  function openModal(event) {
    if (event) event.preventDefault();
    // Use the banner's href for the CTA
    var targetHref = link.getAttribute('href') || '#';
    cta.setAttribute('href', targetHref);

    overlay.classList.add('is-open');
    document.body.classList.add('payday-lock');
    overlay.setAttribute('aria-hidden', 'false');

    // focus management
    lastFocused = document.activeElement;
    closeB.focus();

    // trap focus (simple)
    document.addEventListener('keydown', onKeydown);
  }

  // Close modal
  function closeModal() {
    overlay.classList.remove('is-open');
    document.body.classList.remove('payday-lock');
    overlay.setAttribute('aria-hidden', 'true');
    document.removeEventListener('keydown', onKeydown);
    if (lastFocused) lastFocused.focus();
  }

  function onKeydown(e) {
    if (e.key === 'Escape') closeModal();
  }

  // Clicks
  link.addEventListener('click', openModal);
  closeB.addEventListener('click', closeModal);

  // Click outside modal content to close
  overlay.addEventListener('click', function(e) {
    if (e.target === overlay) closeModal();
  });
})();
