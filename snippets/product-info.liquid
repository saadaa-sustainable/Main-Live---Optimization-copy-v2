<script>
(() => {
  const el = document.getElementById('saveLineBadge-{{ product.id }}');
  if (!el) return;

  const inr = new Intl.NumberFormat('en-IN', { maximumFractionDigits: 0 });

  function render(priceCents, compareCents){
    const p = Number(priceCents)   || 0;
    const c = Number(compareCents) || 0;
    const diff = c - p; // cents/paise

    if (diff > 50) { // show only if ~₹1+
      const rupees = Math.round(diff / 100);
      el.textContent = `Save ₹${inr.format(rupees)} instantly`;
      el.hidden = false;
    } else {
      el.hidden = true;
    }
    el.dataset.priceCents   = p;
    el.dataset.compareCents = c;
  }

  // Initial render from Liquid-provided data attributes
  render(el.dataset.priceCents, el.dataset.compareCents);

  // Helper to update from a Shopify variant object (amounts are in cents)
  function updateFromVariant(variant){
    if (!variant) return;
    render(variant.price, variant.compare_at_price);
  }

  // Theme events (Dawn/OS 2.0 and many others)
  document.addEventListener('variant:change', e => updateFromVariant(e.detail && e.detail.variant));
  document.addEventListener('product:variant-change', e => updateFromVariant(e.detail && e.detail.variant));

  // Fallback: observe the variant select and use product JSON
  const variantSelect = document.querySelector('form[action*="/cart/add"] [name="id"]');
  const jsonEl = document.querySelector('script[id^="ProductJson-"]') || document.getElementById('ProductJson-Manual');
  let productJson = null;
  if (jsonEl && jsonEl.textContent) { try { productJson = JSON.parse(jsonEl.textContent); } catch(e){} }

  if (variantSelect && productJson && Array.isArray(productJson.variants)) {
