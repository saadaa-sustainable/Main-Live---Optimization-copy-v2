<script>
  const total_price_element = document.querySelector(".swp_total_price");
let checked_checkboxes = Array.from(
  document.getElementsByClassName("swp_checkbox")
);
let all_select = document.querySelectorAll(".option_wrapper .select_box");
let all_option_wrapper = document.querySelectorAll(".option_wrapper");

document
  .getElementsByClassName("swp_items_wrapper")[0]
  .addEventListener("change", function () {
    all_select.forEach((ele) => (ele.style.color = "black"));
    checked_checkboxes = Array.from(
      document.getElementsByClassName("swp_checkbox")
    ).filter((e) => e.checked);
    const total_price = checked_checkboxes.reduce(
      (totalPrice, checkbox) => totalPrice + Number(checkbox.dataset.price),
      0
    );
    total_price_element.innerHTML = `₹ ${total_price}`;
  });

all_select.forEach((element) =>
  element.addEventListener("click", function () {
    all_option_wrapper.forEach((ele) => {
      if(ele != element.parentElement){
      ele.classList.remove("open")
      }
    });
    element.parentElement.classList.toggle("open");
    element.style.color = "black";
  })
);
document.querySelectorAll(".swp_p_options li").forEach((ele) =>
  ele.addEventListener("click", function () {
    ele.parentElement.previousElementSibling.dataset.value = ele.dataset.value;
    ele.parentElement.previousElementSibling.firstElementChild.innerHTML =
      ele.dataset.variant;
    ele.closest(".option_wrapper").classList.remove("open");
  })
);
const swp_act_button = document.querySelector(".swp_button_wrapper button");
swp_act_button.addEventListener("click", function () {
  let goToaddToCart = false;
  const selected_variant = [];
  for (let index = 0; index < checked_checkboxes.length; index++) {
    const element = checked_checkboxes[index];
    let select =
      element.parentElement.parentElement.querySelector(".select_box");
    console.dir(select);
    if (select.dataset.value == "notSelected") {
      select.style.color = "red";
      window.scrollBy(
        0,
        select.parentElement.offsetTop - swp_act_button.offsetTop
      );
      goToaddToCart = true;
      break;
    } else {
      selected_variant.push(Number(select.dataset.value));
    }
  }
  if (goToaddToCart || selected_variant.length == 0) {
    return;
  }
  let formData = {
    items: selected_variant.map((item) => ({
      id: item,
      quantity: 1,
    })),
  };
  fetch(window.Shopify.routes.root + "cart/add.js", {
    method: "POST",
    headers: {
      "Content-Type": "application/json",
    },
    body: JSON.stringify(formData),
  })
    .then((response) => {
      if (response.ok) {
        if (openGokwikSideCart) {
          openGokwikSideCart();
          return;
        }
      }
    })
    .catch((error) => {
      console.error("Error:", error);
    });
});
</script>
